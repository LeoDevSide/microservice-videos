{
  "extends": [
    "@rocketseat/eslint-config/node"
  ],
  "overrides": [
    {
      "files": [
        "**/*.spec.ts",
        "**/*.int-spec.ts",
        "**/*.e2e-spec.ts"
      ],
      "env": {
        "jest": true
      }
    }
  ],
  "rules": {
    "no-useless-constructor": "off",
    "dot-notation": "off"
  }
}